function sendDataToDestination() {
  // URL de la hoja de destino
  var destinationSheetUrl = 'https://docs.google.com/spreadsheets/d/1s-Ic1Nmwc34Y3QzByqwJp-pxrx8U08ywVf9rF6D6VoI/edit';
  
  // Abrir la hoja de destino
  var destinationSS = SpreadsheetApp.openByUrl(destinationSheetUrl);
  var destinationSheet = destinationSS.getSheets()[0]; // Asegurarse de seleccionar la primera hoja

  // Obtener los datos de la hoja origen (la que está activa)
  var sourceSheet = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet();
  var sourceDataA1 = sourceSheet.getRange('A1').getValue();
  var sourceDataB1 = sourceSheet.getRange('B1').getValue();

  // Encontrar la siguiente fila vacía en la columna C de la hoja de destino
  var destRow = findNextEmptyRow(destinationSheet, 3);

  // Escribir los valores en la hoja de destino en C y D
  destinationSheet.getRange(destRow, 3).setValue(sourceDataA1); // Columna C
  destinationSheet.getRange(destRow, 4).setValue(sourceDataB1); // Columna D
}

function findNextEmptyRow(sheet, column) {
  var columnValues = sheet.getRange(1, column, sheet.getMaxRows()).getValues();
  for (var i = 0; i < columnValues.length; i++) {
    if (!columnValues[i][0]) {
      return i + 1; // Las filas en Google Sheets son 1-based
    }
  }
  return columnValues.length + 1; // Si todas las filas están llenas, devuelve la siguiente fila
}
